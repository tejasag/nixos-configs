#!/run/current-system/sw/bin/bash

# wallpaper
nitrogen --set-scaled --random ~/wallpapers/nord/nord-selected &

~/.dotfiles/config/herbstluftwm/configs/theme
~/.dotfiles/config/herbstluftwm/configs/keybinds
~/.dotfiles/config/herbstluftwm/configs/setup

# Polybar
polybar -q main -c "~/.dotfiles/config/polybar/config.ini" &

# Conky
conky -c ~/.dotfiles/config/conky/nord.conkyrc

